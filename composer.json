{
    "name": "cgdsoftware/impersonate",
    "description": "User impersonation dependency for Laravel Enso",
    "keywords": [
        "cgdsoftware",
        "impersonate",
        "impersonate-users"
    ],
    "homepage": "https://github.com/cgdsoftware/impersonate",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://cgdsoftware.com",
            "role": "Developer"
        }
    ],
    "require": {
        "cgdsoftware/core": "^8.0",
        "cgdsoftware/migrator": "^2.1",
        "cgdsoftware/dynamic-methods": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Impersonate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Impersonate\\AppServiceProvider",
                "LaravelEnso\\Impersonate\\AuthServiceProvider"
            ],
            "aliases": {}
        }
    }
}
